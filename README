  /usr/share/icons/gnome/192x192/*/*
  /usr/share/icons/gnome/24x24/*/*
  /usr/share/icons/gnome/32x32/*/*
  /usr/share/icons/gnome/36x36/*/*
  /usr/share/icons/gnome/48x48/*/*
  /usr/share/icons/gnome/72x72/*/*
  /usr/share/icons/gnome/96x96/*/*
  /usr/share/icons/gnome/index.theme
  /usr/share/icons/gnome/scalable/*/*
  /usr/share/pkgconfig/gnome-icon-theme.pc

